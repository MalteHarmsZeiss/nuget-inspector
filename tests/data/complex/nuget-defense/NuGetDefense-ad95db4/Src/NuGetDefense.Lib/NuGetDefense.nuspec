<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>NuGetDefense.Lib</id>
        <title>NuGetDefense.Lib</title>
        <version>3.0.8</version>
        <authors>Curtis Carter</authors>
        <owners>Curtis Carter</owners>
        <projectUrl>https://digitalcoyote.github.io/NuGetDefense/</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <summary>NuGetDefense ~ Check for Known Vulnerabilities at Build</summary>
        <description>Library containing the functionality of the NuGetDefense Global Tool for programmtic usage.
        </description>
        <releaseNotes>https://github.com/digitalcoyote/NuGetDefense/releases</releaseNotes>
        <repository type="git" url="https://github.com/digitalcoyote/NuGetDefense.git"/>
        <license type="expression">MIT</license>
        <icon>images\icon.png</icon>
        <tags>Security</tags>
        <dependencies>
            <group targetFramework="net5.0">
                <dependency id="ByteDev.DotNet" version="7.1.0" exclude="Build,Analyzers"/>
                <dependency id="NuGet.Versioning" version="5.10.0" exclude="Build,Analyzers"/>
                <dependency id="NuGetDefense.Core" version="2.0.2" exclude="Build,Analyzers"/>
                <dependency id="NuGetDefense.NVD" version="2.0.5" exclude="Build,Analyzers"/>
                <dependency id="NuGetDefense.OSSIndex" version="2.0.2" exclude="Build,Analyzers"/>
                <dependency id="Serilog.Sinks.Console" version="4.0.0" exclude="Build,Analyzers"/>
                <dependency id="Serilog.Sinks.File" version="5.0.0" exclude="Build,Analyzers"/>
                <dependency id="System.CommandLine" version="2.0.0-beta1.21308.1" exclude="Build,Analyzers"/>
                <dependency id="System.Text.Json" version="5.0.2" exclude="Build,Analyzers"/>
            </group>
        </dependencies>
    </metadata>

    <files>
        <file src="../NuGetDefense/icon.png" target="images\"/>
        <file src="bin/Release/net5.0/NuGetDefense.Lib.dll" target="lib/net5.0/NuGetDefense.Lib.dll"/>
    </files>
</package>